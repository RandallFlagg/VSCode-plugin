{
    "name": "rax-panel",
    "displayName": "rax-panel",
    "description": "rax, weex",
    "version": "0.0.1",
    "publisher": "luf",
    "engines": {
        "vscode": "^1.14.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.rax-web",
        "onCommand:extension.rax-weex"
