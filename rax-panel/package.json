{
  "name": "rax-panel",
  "displayName": "rax-panel",
  "description": "rax, weex",
  "version": "0.0.1",
  "publisher": "luf",
  "engines": {
    "vscode": "^1.14.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.rax-init",
    "onCommand:extension.rax-web",
    "onCommand:extension.rax-weex",
    "onCommand:extension.rax-publish"
  ],
  "main": "./src/index",
  "contributes": {
    "commands": [
      {
        "command": "extension.rax-init",
        "title": "init",
        "category": "Rax"
      },
      {
        "command": "extension.rax-web",
        "title": "web",
        "category": "Rax"
      },
      {
        "command": "extension.rax-weex",
        "title": "weex",
        "category": "Rax"
      },
      {
        "command": "extension.rax-publish",
        "title": "publish",
        "category": "Rax"
      }
    ]
  },
  "scripts": {
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^2.0.3",
    "vscode": "^1.0.0",
    "mocha": "^2.3.3",
    "eslint": "^3.6.0",
    "@types/node": "^6.0.40",
    "@types/mocha": "^2.2.32"
  },
  "dependencies": {
    "detect-port": "^1.2.1",
    "ip": "^1.1.5",
    "rax-cli": "^0.4.7",
    "weex-devtool": "^0.2.80",
    "weex-toolkit": "^1.0.5"
  }
